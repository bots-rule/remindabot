{
  "name": "remindabot",
  "version": "1.0.0",
  "description": "A github bot to remind you of stuff",
  "author": "Vaibhav Vishal <vaibhav.hrt@gmail.com>",
  "license": "ISC",
  "repository": "https://github.com/bots-rule/remindabot.git",
  "homepage": "https://github.com/bots-rule/remindabot",
  "bugs": "https://github.com/bots-rule/remindabot/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app",
    "github-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "eslint **/*.js",
    "test": "jest",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "probot": "^7.2.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "jest": "^24.0.0",
    "nock": "^10.0.0",
    "nodemon": "^1.17.2",
    "smee-client": "^1.0.2"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
